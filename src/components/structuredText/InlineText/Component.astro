---
import { StructuredText } from '@datocms/astro';
import NoParagraph from './NoParagraph.astro';
import type { StructuredTextGraphQlResponseRecord, StructuredTextPropTypes } from 'react-datocms';

type Props = {
  data: StructuredTextPropTypes<
    StructuredTextGraphQlResponseRecord,
    StructuredTextGraphQlResponseRecord
  >['data'];
};

const { data } = Astro.props;
---

<StructuredText data={data} nodeOverrides={{ paragraph: NoParagraph }} />
