---
import { Svg } from '~/components/Svg';
import s from './style.module.css';
import { parse } from 'node-html-parser';

interface Props {
  largeBullet?: boolean;
  style: 'bad' | 'good';
}

const { largeBullet, style } = Astro.props;
---

<ul class:list={[s.root, largeBullet && s.isLarge]} data-style={style}>
  <slot />
</ul>
