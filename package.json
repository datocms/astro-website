{
  "name": "datocms-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "[ -z \"${INSIDE_DOCKER}\" ] && npx simple-git-hooks || true",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "decrypt-env": "npx kavo files:decrypt",
    "generate-schema": "dotenv -e .env.development -c -- bash -c 'gql.tada generate schema https://graphql.datocms.com --header \"X-Exclude-Invalid: true\" --header \"Authorization: $DATOCMS_API_TOKEN\"'",
    "generate-cma-schema": "dotenv -e .env.development -c -- bash -c 'npx datocms schema:generate src/lib/cma-schema.ts'",
    "invalidate-data-sources": "curl -i -X POST \"https://$DRAFT_MODE_HOSTNAME/api/data-sources-invalidate?token=$SECRET_API_TOKEN\"",
    "invalidate-everything": "curl -i -X POST \"https://api.fastly.com/service/$FASTLY_SERVICE_ID/purge_all\" -H \"Fastly-Key: $FASTLY_KEY\" -H \"Accept: application/json\"",
    "reindex-site-search": "npx datocms cma:call searchIndexes trigger 34759",
    "lint": "npx prettier --check .",
    "format": "npx prettier . --write",
    "test": "vitest",
    "serve": "dotenv -e .env.production -c -- bash -c 'node ./dist/server/entry.mjs'",
    "start-prod": "npm run astro build && npm run serve"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.0",
    "@astrojs/check": "^0.9.6",
    "@astrojs/node": "^9.5.1",
    "@astrojs/react": "^4.4.2",
    "@datocms/astro": "^0.6.8",
    "@datocms/cda-client": "^0.2.7",
    "@datocms/cli": "^3.1.15",
    "@datocms/cma-client": "^5.2.0",
    "@dkvz/img-lightbox": "^0.2.0",
    "@expressive-code/plugin-line-numbers": "^0.38.3",
    "@flex-development/unist-util-types": "^1.6.1",
    "@floating-ui/dom": "^1.7.4",
    "@mailerlite/mailerlite-nodejs": "^1.5.0",
    "@mozilla/readability": "^0.6.0",
    "@mux/mux-player": "^2.9.1",
    "@slack/web-api": "^7.13.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@wordpress/shortcode": "^4.7.0",
    "astro": "^5.16.4",
    "astro-expressive-code": "^0.38.3",
    "basecamp-client": "^1.0.7",
    "classnames": "^2.5.1",
    "country-list": "^2.3.0",
    "custom-prism-react-renderer": "^1.0.6",
    "date-fns": "^4.1.0",
    "datocms-react-ui": "^2.0.19",
    "datocms-structured-text-generic-html-renderer": "^5.1.7",
    "datocms-structured-text-to-html-string": "^5.1.7",
    "datocms-structured-text-to-plain-text": "^5.1.7",
    "datocms-structured-text-utils": "^5.1.7",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "gql.tada": "^1.9.0",
    "graphql": "^16.12.0",
    "hast-util-select": "^6.0.4",
    "hast-util-to-string": "^3.0.1",
    "html-parsed-element": "^0.4.1",
    "http-status-ts": "^2.0.1",
    "ky": "^1.14.0",
    "linkedom": "^0.18.12",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.2.4",
    "mdast-util-to-string": "^4.0.0",
    "nanoid": "^5.1.6",
    "next-mdx-remote": "^5.0.0",
    "node-html-parser": "^7.0.1",
    "openai": "^6.10.0",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-datocms": "^7.2.4",
    "react-dom": "^18.3.1",
    "react-dom-confetti": "^0.2.0",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-recaptcha-hook": "^1.2.3",
    "react-simple-maps": "^3.0.0",
    "react-spring": "^9.7.4",
    "react-textarea-autosize": "^8.5.9",
    "react-transition-group": "^4.4.5",
    "remark-directive": "^4.0.0",
    "remove-markdown": "^0.6.2",
    "rollbar": "^2.26.5",
    "rss": "^1.2.2",
    "satori": "^0.18.3",
    "satori-html": "^0.3.2",
    "serialize-error": "^12.0.0",
    "sharp": "^0.34.5",
    "sitemap": "^9.0.0",
    "striptags": "^3.2.0",
    "style-object-to-css-string": "^1.1.3",
    "svgo": "^3.3.2",
    "turndown": "^7.2.2",
    "typedoc": "^0.26.7",
    "unist-util-visit": "^5.0.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.6",
    "@datocms/cubo": "^0.2.51",
    "@types/country-list": "^2.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-simple-maps": "^3.0.6",
    "@types/rss": "^0.0.32",
    "@types/turndown": "^5.0.6",
    "@types/wordpress__shortcode": "^2.3.6",
    "dotenv-cli": "^11.0.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-nested": "4.2.3",
    "prettier-plugin-astro": "^0.14.1",
    "rfs": "^10.0.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.6.0",
    "vite-plugin-bundlesize": "^0.1.0",
    "vitest": "^3.0.9"
  },
  "simple-git-hooks": {
    "pre-commit": "FILES=$(git diff --cached --name-only --diff-filter=ACMR) && echo \"$FILES\" | xargs npx prettier --ignore-unknown --write && git add $FILES"
  },
  "engines": {
    "node": "20"
  }
}
